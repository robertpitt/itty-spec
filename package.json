{
  "name": "itty-spec",
  "version": "0.1.0",
  "description": "",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsdown",
    "format": "oxfmt src/ examples/ tests/",
    "format:check": "oxfmt --check",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:types": "vitest --typecheck",
    "prepublishOnly": "npm run build",
    "prepare": "husky"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/bun": "^1.3.4",
    "@types/node": "^25.0.1",
    "husky": "^9.1.7",
    "oxfmt": "^0.17.0",
    "tsdown": "^0.17.0",
    "typescript": "^5.0.0",
    "vitest": "^4.0.15",
    "zod": "v4"
  },
  "dependencies": {
    "@standard-schema/spec": "^1.0.0",
    "itty-router": "^5"
  }
}
