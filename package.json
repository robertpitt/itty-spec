{
  "name": "itty-spec",
  "version": "0.2.9",
  "description": "Type-safe contract first itty-router",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./contract": {
      "types": "./dist/contract.d.mts",
      "require": "./dist/contract.cjs",
      "import": "./dist/contract.mjs"
    },
    "./openapi": {
      "types": "./dist/openapi/index.d.mts",
      "require": "./dist/openapi/index.cjs",
      "import": "./dist/openapi/index.mjs"
    },
    "./router": {
      "types": "./dist/router.d.mts",
      "require": "./dist/router.cjs",
      "import": "./dist/router.mjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsdown",
    "format": "oxfmt src/ examples/ tests/",
    "format:check": "oxfmt --check src/ examples/ tests/",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:types": "vitest --typecheck",
    "prepublishOnly": "pnpm run build",
    "prepare": "husky",
    "publish:npm": "pnpm publish --access public",
    "docs:dev": "vitepress dev docs-site",
    "docs:build": "vitepress build docs-site",
    "docs:preview": "vitepress preview docs-site"
  },
  "keywords": [
    "itty-router",
    "api",
    "contract",
    "typescript",
    "validation",
    "schema",
    "zod",
    "valibot",
    "cloudflare-workers"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:robertpitt/itty-spec.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@braintree/sanitize-url": "^7.1.1",
    "@types/bun": "^1.3.4",
    "@types/node": "^25.0.1",
    "@valibot/to-json-schema": "^1.5.0",
    "@vitest/coverage-v8": "^4.0.15",
    "@whatwg-node/server": "^0.10.17",
    "cytoscape": "^3.33.1",
    "cytoscape-cose-bilkent": "^4.1.0",
    "dayjs": "^1.11.19",
    "debug": "^4.4.3",
    "husky": "^9.1.7",
    "mermaid": "^11.12.2",
    "oxfmt": "^0.17.0",
    "sass": "^1.97.1",
    "tsdown": "^0.17.0",
    "tsx": "^4.21.0",
    "typescript": "^5.0.0",
    "valibot": "^1.2.0",
    "vitepress": "next",
    "vitepress-plugin-mermaid": "^2.0.17",
    "vitest": "^4.0.15",
    "zod": "v4",
    "zod-openapi": "^4.0.0"
  },
  "dependencies": {
    "@standard-community/standard-json": "^0.3.5",
    "@standard-community/standard-openapi": "^0.2.0",
    "@standard-schema/spec": "^1.0.0",
    "itty-router": "^5",
    "openapi-types": "=12.1.3"
  }
}
